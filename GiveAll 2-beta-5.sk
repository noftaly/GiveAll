function GiveAll_init():
	# Variables très importantes : elles régissent les conditions. Ce sont des modèles sur lesquels se calquent les nouvelles conditions
	clear {ga::debug::conds::*}
	clear {ga::debug::conds}
	add "msg-same", "msg-contain", "perm", "min-kill", "max-kill", "min-death", "max-death", "min-money", "max-money", "min-co", "max-co", "in-region", "not-in-region", "in-world", "not-in-world", "min-ratio" and "max-ratio" to {ga::debug::conds::*}
	loop {ga::debug::conds::*}:
		set {ga::debug::conds::%loop-value%::isEnable} to false
		set {ga::debug::conds::%loop-value%::value} to "Aucune"
	set {ga::debug::conds::msg-same::description} to "{@msg-same_desc}"
	set {ga::debug::conds::msg-same::fullName} to "{@msg-same_name}"
	set {ga::debug::conds::msg-contain::description} to "{@msg-contain_desc}"
	set {ga::debug::conds::msg-contain::fullName} to "{@msg-contain_name}"
	set {ga::debug::conds::perm::description} to "{@perm_desc}"
	set {ga::debug::conds::perm::fullName} to "{@perm_name}"
	set {ga::debug::conds::min-kill::description} to "{@min-kill_desc}"
	set {ga::debug::conds::min-kill::fullName} to "{@min-kill_name}"
	set {ga::debug::conds::max-kill::description} to "{@max-kill_desc}"
	set {ga::debug::conds::max-kill::fullName} to "{@max-kill_name}"
	set {ga::debug::conds::min-death::description} to "{@min-death_desc}"
	set {ga::debug::conds::min-death::fullName} to "{@min-death_name}"
	set {ga::debug::conds::max-death::description} to "{@max-death_desc}"
	set {ga::debug::conds::max-death::fullName} to "{@max-death_name}"
	set {ga::debug::conds::min-money::description} to "{@min-money_desc}"
	set {ga::debug::conds::min-money::fullName} to "{@min-money_name}"
	set {ga::debug::conds::max-money::description} to "{@max-money_desc}"
	set {ga::debug::conds::max-money::fullName} to "{@max-money_name}"
	set {ga::debug::conds::min-co::description} to "{@min-co_desc}"
	set {ga::debug::conds::min-co::fullName} to "{@min-co_name}"
	set {ga::debug::conds::max-co::description} to "{@max-co_desc}"
	set {ga::debug::conds::max-co::fullName} to "{@max-co_name}"
	set {ga::debug::conds::in-region::description} to "{@in-region_desc}"
	set {ga::debug::conds::in-region::fullName} to "{@in-region_name}"
	set {ga::debug::conds::not-in-region::description} to "{@not-in-region_desc}"
	set {ga::debug::conds::not-in-region::fullName} to "{@not-in-region_name}"
	set {ga::debug::conds::in-world::description} to "{@in-world_desc}"
	set {ga::debug::conds::in-world::fullName} to "{@in-world_name}"
	set {ga::debug::conds::not-in-world::description} to "{@not-in-world_desc}"
	set {ga::debug::conds::not-in-world::fullName} to "{@not-in-world_name}"
	set {ga::debug::conds::min-ratio::description} to "{@min-ratio_desc}"
	set {ga::debug::conds::min-ratio::fullName} to "{@min-ratio_name}"
	set {ga::debug::conds::max-ratio::description} to "{@max-ratio_desc}"
	set {ga::debug::conds::max-ratio::fullName} to "{@max-ratio_name}"
	loop all players:
		if {ga::player::%loop-player%::dispMode} is not set:
			set {ga::player::%loop-player%::dispMode} to "GroupsAndGa"
	set {ga::debug::indexToHuman::time} to "Durée"
	set {ga::debug::indexToHuman::countdown} to "Compte à rebours"
	set {ga::debug::indexToHuman::group} to "Groupe"
	set {ga::debug::indexToHuman::status} to "Statut"
	set {ga::debug::indexToHuman::droptype} to "Type de don des items"

function GiveAll_newGiveAll(ga: text, group: text, time: integer, countdown: integer, drop: text, creator: text, date: text, items: items):
	add {_ga} to {ga::list::*}
	set {ga::specs::%{_ga}%::time} to {_time}
	set {ga::specs::%{_ga}%::countdown} to {_countdown}
	set {ga::specs::%{_ga}%::group} to {_group}
	set {ga::specs::%{_ga}%::status} to "Stop"
	set {ga::specs::%{_ga}%::droptype} to {_drop}
	set {ga::specs::%{_ga}%::infos::date} to {_date}
	set {ga::specs::%{_ga}%::infos::creator} to {_creator}
	loop {ga::debug::conds::*}:
		add loop-value to {ga::specs::%{_ga}%::conditions::*}
	loop {_items::*}:
		set {_itm} to loop-value
		add {_itm} to {ga::specs::%{_ga}%::items::*}

function GiveAll_newGroup(grp: text, time: integer, countdown: integer, creator: text, date: text):
	add {_grp} to {ga::glist::*}
	set {ga::gspecs::%{_grp}%::time} to {_time}
	set {ga::gspecs::%{_grp}%::countdown} to {_countdown}
	set {ga::gspecs::%{_grp}%::infos::date} to {_date}
	set {ga::gspecs::%{_grp}%::infos::creator} to {_creator}

function GiveAll_deleteGroup(grp: text):
	clear {ga::gspecs::%{_grp}%::givealls::*} # On clear son contenu
	clear {ga::gspecs::%{_grp}%::*} # On clear ses specs
	remove {_grp} from {ga::gspecs::*} # On l'enlève de la liste des specs. Maintenant cette liste a un trou mais on s'en fiche (car c'est juste une liste de stockage de données)
	remove {_grp} from {ga::glist::*} # On l'enlève de la liste des groupes. Mais maintenant cette liste a un trou mais on s'en fiche pas (car on va se servir de cette liste pour afficher les GiveAlls dans le GUI, et on veut pas qu'il y ait de trous)
	set {_l::*} to {ga::glist::*} # Du coup on recréé la liste mais sans trou
	clear {ga::glist::*}
	add {_l::*} to {ga::glist::*}

function GiveAll_deleteGiveAll(ga: text):
	loop {ga::glist::*}:
		GiveAll_removeGaFromGroup({_ga}, loop-value)
	clear {ga::specs::%{_ga}%::*} # On clear ses specs
	remove {_ga} from {ga::specs::*} # On l'enlève de la liste des specs. Maintenant cette liste a un trou mais on s'en fiche (car c'est juste une liste de stockage de données)
	remove {_ga} from {ga::list::*} # On l'enlève de la liste des GiveAlls. Mais maintenant cette liste a un trou mais on s'en fiche pas (car on va se servir de cette liste pour afficher les GiveAlls dans le GUI, et on veut pas qu'il y ait de trous)
	set {_l::*} to {ga::list::*} # Du coup on recréé la liste mais sans trou
	clear {ga::list::*}
	add {_l::*} to {ga::list::*}

function GiveAll_stop(ga: text):
	clear {ga::current::%{_ga}%::*}
	set {ga::specs::%{_ga}%::status} to "Stop"
	remove {_ga} from {ga::current::*}

function GiveAll_purgeGiveAll():
	loop {ga::glist::*}:
		loop {ga::list::*}:
			GiveAll_removeGaFromGroup(loop-value-2, loop-value-1)
	clear {ga::list::*}
	clear {ga::specs::*}

function GiveAll_purgeGroup():
	clear {ga::glist::*}
	clear {ga::gspecs::*}

function GiveAll_renameGiveAll(old: text, new: text):
	set {_stime} to {ga::specs::%{_old}%::time}
	set {_specs::status} to {ga::specs::%{_old}%::status}
	set {_specs::group} to {ga::specs::%{_old}%::group}
	set {_specs::countdown} to {ga::specs::%{_old}%::countdown}
	set {_specs::drop} to {ga::specs::%{_old}%::droptype}
	set {_specs::infos::date} to {ga::specs::%{_old}%::infos::date}
	set {_specs::infos::creator} to {ga::specs::%{_old}%::infos::creator}
	set {_specs::conditions::*} to {ga::specs::%{_old}%::conditions::*}
	set {_specs::items::*} to {ga::specs::%{_old}%::items::*}
	GiveAll_deleteGiveAll({_old})
	GiveAll_newGiveAll({_new}, {_specs::group}, {_stime}, {_specs::countdown}, {_specs::drop}, {_specs::infos::creator}, {_specs::infos::date},{_specs::items::*})
	set {ga::specs::%{_new}%::status} to {_specs::status}

function GiveAll_copyItems(import: text, export: text):
	add {ga::specs::%{_import}%::items::*} to {ga::specs::%{_export}%::items::*}

function GiveAll_setDropType(ga: text, type: text):
	if {_type} is "drop" or "give":
		set {ga::specs::%{_ga}%::droptype} to {_type}

function GiveAll_removeItem(ga: text, i: item):
	remove {_i} from {ga::specs::%{_ga}%::items::*}
	set {_item1::*} to {ga::specs::%{_ga}%::items::*}
	clear {ga::specs::%{_ga}%::items::*}
	wait 1 tick
	set {ga::specs::%{_ga}%::items::*} to {_item1::*}

function GiveAll_addCondition(ga: text, cond: text, val: text):
	set {ga::specs::%{_ga}%::conditions::%{_cond}%::isEnable} to true
	set {ga::specs::%{_ga}%::conditions::%{_cond}%::fullName} to {ga::debug::conds::%{_cond}%::fullName}
	set {ga::specs::%{_ga}%::conditions::%{_cond}%::description} to {ga::debug::conds::%{_cond}%::description}
	set {ga::specs::%{_ga}%::conditions::%{_cond}%::value} to {_val}

function GiveAll_removeCondition(ga: text, cond: text):
	set {ga::specs::%{_ga}%::conditions::%{_cond}%::isEnable} to false

function GiveAll_addGaToGroup(ga: text, grp: text):
	add {_ga} to {ga::gspecs::%{_grp}%::givealls::*}

function GiveAll_removeGaFromGroup(ga: text, grp: text):
	remove {_ga} from {ga::gspecs::%{_grp}%::givealls::*}
	set {_g::*} to {ga::gspecs::%{_grp}%::givealls::*}
	clear {ga::gspecs::%{_grp}%::givealls::*}
	wait 1 tick
	set {ga::gspecs::%{_grp}%::givealls::*} to {_g::*}

function GiveAll_clearGroup(grp: text):
	clear {ga::gspecs::%{_grp}%::givealls::*}

