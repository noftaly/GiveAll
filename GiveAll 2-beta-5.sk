options:
	version: 2.0-beta-5 #!#! Ne pas changer !#!#
function updateCheck(p: player):
	set {_newver} to text from "https://raw.githubusercontent.com/noftaly/GiveAll/master/version"
	if {_newver} isn't "{@version}":
		set {_new} to text from "https://raw.githubusercontent.com/noftaly/GiveAll/master/Nouveautes"
		message "&8[&4&m-------------------------&c&m>&6&m+&c&m<&4&m-------------------------&8]" to {_p}
		message "&r" to {_p}
		message "{@Prefix} &aLe skript de &2GiveAll&a n'est pas à jour ! &f&o({@version} / %{_newver}%)" to {_p}
		message "{@Prefix} &aNouveautés &f&o(de la version %{_newver}%) &a: &2%uncolored {_new}%" to {_p}
		message "{@Prefix} &aLien de téléchargement :&2 https://bit.ly/2tLQ1nR" to {_p}
		message "&r" to {_p}
		message "&8[&4&m-------------------------&c&m>&6&m+&c&m<&4&m-------------------------&8]" to {_p}
	else:
		message "{@Prefix} &aLe skript est à jour ! &a&oBon jeu !" to {_p}

