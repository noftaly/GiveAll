function GiveAll_debug(fix: boolean=true) :: strings:
	set {ga::debug::minGiveTime} to {@MinGiveTime}
	set {ga::debug::maxGiveTime} to {@MaxGiveTime}
	if {@MinGiveTime} is not an integer:
		add "Le temps de give minimum n'est pas un entier.%("" [RESOLU] (Valeur par défaut: 1)"") if {_fix} is true, else """"%" to {_errors::*}
		if {_fix} is true:
			set {ga::debug::minGiveTime} to 1
	if {@MaxGiveTime} is not an integer:
		add "Le temps de give maximum n'est pas un entier.%(""  [RESOLU] (Valeur par défaut: 3)"") if {_fix} is true, else """"%" to {_errors::*}
		if {_fix} is true:
			set {ga::debug::maxGiveTime} to 3
	if {ga::debug::minGiveTime} < 0:
		set {_a} to abs({ga::debug::minGiveTime})
		set {_b} to "  [RESOLU] (Valeur absolue: %{_a}%)"
		add "Le temps de give minimum est inférieur à 0.%{_b} if {_fix} is true, else """"%" to {_errors::*}
		if {_fix} is true:
			set {ga::debug::minGiveTime} to abs({ga::debug::minGiveTime})
	if {ga::debug::maxGiveTime} < 0:
		set {_a} to abs({ga::debug::maxGiveTime})
		set {_b} to "  [RESOLU] (Valeur absolue: %{_a}%)"
		add "Le temps de give maximum est inférieur à 0.%{_b} if {_fix} is true, else """"%" to {_errors::*}
		if {_fix} is true:
			set {ga::debug::maxGiveTime} to abs({ga::debug::maxGiveTime})
	if {ga::debug::minGiveTime} > {ga::debug::maxGiveTime}:
		add "Le temps de give minimum est supérieur au temps de give maximum.%(""  [RESOLU] (Inversement des valeurs)"") if {_fix} is true, else """"%" to {_errors::*}
		if {_fix} is true:
			set {_max} to {ga::debug::maxGiveTime}
			set {ga::debug::maxGiveTime} to {ga::debug::minGiveTime}
			set {ga::debug::minGiveTime} to {_max}
	return {_errors::*}

function GiveAll_gaExist(ga: text) :: boolean:
	loop {ga::list::*}:
		if loop-value is {_ga}:
			return true
	return false

function GiveAll_grpExist(grp: text) :: boolean:
	loop {ga::glist::*}:
		if loop-value is {_grp}:
			return true
	return false

function GiveAll_isInProgress(ga:text) :: boolean:
	if {ga::current::%{_ga}%::status} is "Countdown" or "In progress":
		return true
	return false

function GiveAll_current() :: texts:
	return {ga::current::*}

function GiveAll_hasConditions(ga: text) :: boolean:
	loop {ga::specs::%{_ga}%::conditions::*}:
		if {ga::specs::%{_ga}%::conditions::%loop-value%::isEnable} is true:
			return true
	return false

function GiveAll_activeConditions(ga: text) :: integer:
	set {_c} to 0
	loop {ga::specs::%{_ga}%::conditions::*}:
		if {ga::specs::%{_ga}%::conditions::%loop-value%::isEnable} is true:
			add 1 to {_c}
	return {_c}

function GiveAll_isBlacklist(p: text, ga: text) :: boolean:
	loop {ga::specs::%{_ga}%::blacklist::*}:
		if loop-value is {_p}:
			return true
	return false

function GiveAll_isLastGiveAll() :: boolean:
	loop {ga::list::*}:
		return false
	return true

function GiveAll_playerCanReceiveItems(p: player, ga: text) :: boolean:
	# Si il est blacklist
	if GiveAll_isBlacklist({_p}, {_ga}) is true:
		return false
	# Si le giveall n'a aucune condition
	if GiveAll_hasConditions({_ga}) is not true:
		return true
	if {ga::specs::%{_ga}%::conditions::msg-same::isEnable} is true:
		if {ga::player::%{_p}%::cond::msg-same::%{_ga}%} < 1:
			return false
	if {ga::specs::%{_ga}%::conditions::msg-contain::isEnable} is true:
		if {ga::player::%{_p}%::cond::msg-contain::%{_ga}%} is not set:
			set {ga::player::%{_p}%::cond::msg-contain::%{_ga}%} to 0
		if {ga::player::%{_p}%::cond::msg-contain::%{_ga}%} < 1:
			return false
	if {ga::specs::%{_ga}%::conditions::perm::isEnable} is true:
		if {_p} doesn't have permission "%{ga::specs::%{_ga}%::conditions::perm::value}%":
			return false
	if {ga::specs::%{_ga}%::conditions::min-kill::isEnable} is true:
		if {ga::player::%{_p}%::cond::kill} < ({ga::specs::%{_ga}%::conditions::min-kill::value} parsed as integer):
			return false
	if {ga::specs::%{_ga}%::conditions::max-kill::isEnable} is true:
		if {ga::player::%{_p}%::cond::kill} > ({ga::specs::%{_ga}%::conditions::max-kill::value} parsed as integer):
			return false
	if {ga::specs::%{_ga}%::conditions::min-death::isEnable} is true:
		if {ga::player::%{_p}%::cond::death} < ({ga::specs::%{_ga}%::conditions::min-death::value} parsed as integer):
			return false
	if {ga::specs::%{_ga}%::conditions::max-death::isEnable} is true:
		if {ga::player::%{_p}%::cond::death} > ({ga::specs::%{_ga}%::conditions::max-death::value} parsed as integer):
			return false
	if {ga::specs::%{_ga}%::conditions::min-money::isEnable} is true:
		if {_p}'s balance < ({ga::specs::%{_ga}%::conditions::min-money::value} parsed as integer):
			return false
	if {ga::specs::%{_ga}%::conditions::max-money::isEnable} is true:
		if {_p}'s balance > ({ga::specs::%{_ga}%::conditions::max-money::value} parsed as integer):
			return false
	set {_c} to ("%PlayTimeAPI({_p}, ""totalCoTime"", true)%" parsed as time span)
	if {ga::specs::%{_ga}%::conditions::min-co::isEnable} is true:
		set {_v1} to ({ga::specs::%{_ga}%::conditions::min-co::value} parsed as integer)
		set {_v2} to ("%{_v1}% seconds" parsed as time span)
		if {_c} < {_v2}:
			return false
	if {ga::specs::%{_ga}%::conditions::max-co::isEnable} is true:
		set {_v1} to ({ga::specs::%{_ga}%::conditions::max-co::value} parsed as integer)
		set {_v2} to ("%{_v1}% seconds" parsed as time span)
		if {_c} > {_v2}:
			return false
	set {_r::*} to regex split "%region at {_p}%" with " in world ([A-Za-z0-9])\w*"
	if {ga::specs::%{_ga}%::conditions::in-region::isEnable} is true:
		if "%{_r::1}%" is not {ga::specs::%{_ga}%::conditions::in-region::value}:
			return false
	if {ga::specs::%{_ga}%::conditions::not-in-region::isEnable} is true:
		if "%{_r::1}%" is {ga::specs::%{_ga}%::conditions::in-region::value}:
			return false
	if {ga::specs::%{_ga}%::conditions::in-world::isEnable} is true:
		if {_p} is not in world world("%{ga::specs::%{_ga}%::conditions::in-world::value}%"):
			return false
	if {ga::specs::%{_ga}%::conditions::not-in-world::isEnable} is true:
		if {_p} is in world world("%{ga::specs::%{_ga}%::conditions::in-world::value}%"):
			return false
	if {ga::specs::%{_ga}%::conditions::min-ratio::isEnable} is true:
		if ({ga::player::%{_p}%::cond::kill} / {ga::player::%{_p}%::cond::death}) < ({ga::specs::%{_ga}%::conditions::min-ratio::value} parsed as number):
			return false
	if {ga::specs::%{_ga}%::conditions::max-ratio::isEnable} is true:
		if ({ga::player::%{_p}%::cond::kill} / {ga::player::%{_p}%::cond::death}) > ({ga::specs::%{_ga}%::conditions::max-ratio::value} parsed as number):
			return false
	return true

function realPlayer(pl: text) :: boolean:
	set {_p} to {_pl} parsed as offline player
	loop {ga::debug::allplayers::*}:
		if loop-value is {_p}:
			return true
	return false

function contains(sample: texts, scope: text) :: boolean:
	loop {_sample::*}:
		if loop-value is {_scope}:
			return true
	return false

function or(conditions: booleans) :: boolean: # Fonction de Anarchick
	if {_conditions::*} contains true:
		return true
	return false

function and(conditions: booleans) :: boolean: # Fonction de Anarchick
	if {_conditions::*} contains false:
		return false
	return true

