							set {_time} to "&c&l10&r"
							format gui slot 29 of player with 10 of red wool named "{@RemoveTimeByGUI}" to run:
								remove 10 from {ga::specs::%{_ga}%::time}
								if {ga::specs::%{_ga}%::time} < 0:
									set {ga::specs::%{_ga}%::time} to 0
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&6&l1&r"
							format gui slot 30 of player with 1 of orange wool named "{@RemoveTimeByGUI}" to run:
								remove 1 from {ga::specs::%{_ga}%::time}
								if {ga::specs::%{_ga}%::time} < 0:
									set {ga::specs::%{_ga}%::time} to 0
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to {ga::specs::%{_ga}%::time}
							set {_l} to "{@LoreSeeTimeByGUI}"
							set {_lore::*} to {_l} split at "||"
							format gui slot 31 of player with a clock item named "{@SeeTimeByGUI}" with lore {_lore::*} to run with right mouse button:
								set {ga::specs::%{_ga}%::time} to 0
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&e&l1&r"
							format gui slot 32 of player with 1 of yellow wool named "{@AddTimeByGUI}" to run:
								add 1 to {ga::specs::%{_ga}%::time}
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&a&l10&r"
							format gui slot 33 of player with 10 of light green wool named "{@AddTimeByGUI}" to run:
								add 10 to {ga::specs::%{_ga}%::time}
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&2&l60&r"
							format gui slot 34 of player with 60 of green wool named "{@AddTimeByGUI}" to run:
								add 60 to {ga::specs::%{_ga}%::time}
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"


						format gui slot 37, 38, 39, 40, 41, 42 and 43 of player with a red glass pane named "{@NotEnoughPermissionInGUI}"
						if player has permission "{@PermissionChangeCountDown}" or "{@PermissionAll}":
							set {_time} to "&4&l10&r"
							format gui slot 37 of player with 10 of black wool named "{@RemoveCDByGUI}" to run:
								remove 10 from {ga::specs::%{_ga}%::countdown}
								if {ga::specs::%{_ga}%::countdown} < 10:
									set {ga::specs::%{_ga}%::countdown} to 10
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&c&l5&r"
							format gui slot 38 of player with 5 of red wool named "{@RemoveCDByGUI}" to run:
								remove 5 from {ga::specs::%{_ga}%::countdown}
								if {ga::specs::%{_ga}%::countdown} < 10:
									set {ga::specs::%{_ga}%::countdown} to 10
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&6&l1&r"
							format gui slot 39 of player with 1 of orange wool named "{@RemoveCDByGUI}" to run:
								remove 1 from {ga::specs::%{_ga}%::countdown}
								if {ga::specs::%{_ga}%::countdown} < 10:
									set {ga::specs::%{_ga}%::countdown} to 10
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to {ga::specs::%{_ga}%::countdown}
							set {_l} to "{@LoreSeeCDByGUI}"
							set {_lore::*} to {_l} split at "||"
							format gui slot 40 of player with a clock item named "{@SeeCDByGUI}" with lore {_lore::*} to run with right mouse button:
								set {ga::specs::%{_ga}%::countdown} to 10
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&e&l1&r"
							format gui slot 41 of player with 1 of yellow wool named "{@AddCDByGUI}" to run:
								add 1 to {ga::specs::%{_ga}%::countdown}
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&2&l5&r"
							format gui slot 42 of player with 5 of light green wool named "{@AddCDByGUI}" to run:
								add 5 to {ga::specs::%{_ga}%::countdown}
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
							set {_time} to "&a&l10&r"
							format gui slot 43 of player with 10 of green wool named "{@AddCDByGUI}" to run:
								add 10 to {ga::specs::%{_ga}%::countdown}
								wait 1 ticks
								make player execute command "/{@MainCommand} modify %{_ga}%"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Modify}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					   DropType						  #
		#-----------------------------------------------------#
		else if arg-1 is "drop" or "droptype" or "changedroptype":
			if player has permission "{@PermissionChangeDropType}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						set {_ga} to arg-2
						#set {_drop} to {ga::specs::%{_ga}%::droptype}
						if arg-3 is set:
							if arg-3 is "drop" or "give":
								GiveAll_setDropType({_ga}, arg-3)
								set {_drop} to {ga::specs::%{_ga}%::droptype}
								message "{@Prefix} {@SuccessfullyChangedDropType}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_DropType}"
						else:
							message "{@Prefix} {@DropTypeInfo}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_DropType}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					   Settime 						  #
		#-----------------------------------------------------#
		else if arg-1 is "settime" or "setime" or "changetime" or "changet" or "ctime" or "ct":
			if player has permission "{@PermissionChangeTime}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						if contains(GiveAll_current(), arg-2) is not true:
							if arg-3 is set:
								set {_time} to arg-3 parsed as integer
								if {_time} is an integer:
									set {_time} to abs({_time})
									set {ga::specs::%arg-2%::time} to {_time}
									set {_ga} to arg-2
									message "{@Prefix} {@SuccessfullyChangedTime}"
								else:
									message "{@Prefix} {@ErrorPrefix} {@InvalidNumber}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_ChangeTime}"
						else:
							message "{@Prefix} {@CantModifyBecauseGAIsInProgress}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_ChangeTime}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					  SetCountdown 					  #
		#-----------------------------------------------------#
		else if arg-1 is "setcountdown" or "setcd" or "scd" or "changecountdown" or "changecd" or "ccd":
			if player has permission "{@PermissionChangeCountDown}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						if contains(GiveAll_current(), arg-2) is not true:
							if arg-3 is set:
								set {_time} to arg-3 parsed as integer
								if {_time} is an integer:
									set {_time} to abs({_time})
									if {_time} >= 10:
										set {ga::specs::%arg-2%::countdown} to {_time}
										set {_ga} to arg-2
										message "{@Prefix} {@SuccessfullyChangedCD}"
									else:
										message "{@Prefix} {@ErrorPrefix} {@TimeForCDIsTooSmall}"
								else:
									message "{@Prefix} {@ErrorPrefix} {@InvalidNumber}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_ChangeCD}"
						else:
							message "{@Prefix} {@CantModifyBecauseGAIsInProgress}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_ChangeCD}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					   CopyItem 					  #
		#-----------------------------------------------------#
		else if arg-1 is "copyitem" or "copycontent" or "copy":
			if player has permission "{@PermissionCopyContent}" or "{@PermissionAll}":
				if arg-2 is set:
					if arg-3 is set:
						if GiveAll_gaExist(arg-2):
							if GiveAll_gaExist(arg-3):
								if size of {ga::specs::%arg-2%::items::*} is not 0:
									set {_ga1} to arg-2
									set {_ga2} to arg-3
									GiveAll_copyItems(arg-2, arg-3)
									message "{@Prefix} {@SuccessfullyCopied}"
								else:
									message "{@Prefix} {@ErrorPrefix} {@GAIsEmpty}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_CopyContent}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_CopyContent}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					   Duplicate 					  #
		#-----------------------------------------------------#
		else if arg-1 is "duplicate":
			if player has permission "{@PermissionDuplicate}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						set {_ga} to arg-2
						set {_gaNew} to "%{_ga}%-{@DuplicatedGAdefaultName}"
						if GiveAll_gaExist({_gaNew}):
							set {_s} to 2
							while GiveAll_gaExist({_gaNew}):
								set {_gaNew} to "%{_ga}%-{@DuplicatedGAdefaultName}%{_s}%"
								add 1 to {_s}
						set {_date} to now formatted with "{@dateFormat}"
						GiveAll_newGiveAll({_gaNew}, {ga::specs::%{_ga}%::group}, {ga::specs::%{_ga}%::time}, {ga::specs::%{_ga}%::countdown}, {ga::specs::%{_ga}%::droptype}, {_date}, {ga::specs::%{_ga}%::infos::creator}, {ga::specs::%{_ga}%::items::*})
						message "{@Prefix} {@SuccessfullyDuplicated}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Duplicate}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					   Clear 						  #
		#-----------------------------------------------------#
		else if arg-1 is "clear":
			if player has permission "{@PermissionClearGA}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						if contains(GiveAll_current(), arg-2) is not true:
							clear {ga::specs::%arg-2%::items::*}
							set {_ga} to arg-2
							message "{@Prefix} {@SuccessfullyCleared}"
						else:
							message "{@Prefix} {@CantModifyBecauseGAIsInProgress}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Clear}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 					   Condition					  #
		#-----------------------------------------------------#
		else if arg-1 is "condition" or "cond" or "conditions":
			if arg-2 is set:
				if arg-2 is "list" or "all":
					if player has permission "{@PermissionModifyCondition}" or "{@PermissionAll}":
						set {_number} to ((size of {ga::debug::conds::*}) / 2)
						message "{@Prefix} {@ConditionsList}"
						loop {ga::debug::conds::*}:
							message "{@Prefix} {@ComaColorInList}  -{@GAColorInList} <tooltip:{@GAColorInList}%{ga::debug::conds::%loop-value%::fullName}%%nl%&7%{ga::debug::conds::%loop-value%::description}%>%loop-value%"
					else:
						message "{@NoPermission}"
				else if arg-2 is "see" or "info" :
					if player has permission "{@PermissionSeeCondition}" or "{@PermissionAll}":
						if arg-3 is set:
							if GiveAll_gaExist(arg-3):
								set {_ga} to arg-3
								if arg-4 is "gui":
									GiveAll_conditionGUI(player, {_ga})
								else:
									loop {ga::specs::%{_ga}%::conditions::*}:
										if {ga::specs::%{_ga}%::conditions::%loop-value%::isEnable}:
											add 1 to {_c}
									if {_c} >= 1:
										message "{@Prefix} {@SeeCondInfo}"
										loop {ga::specs::%{_ga}%::conditions::*}:
											set {_cond} to loop-value
											if {ga::specs::%{_ga}%::conditions::%{_cond}%::isEnable}:
												set {_c} to {ga::specs::%{_ga}%::conditions::%{_cond}%::fullName}
												set {_v} to {ga::specs::%{_ga}%::conditions::%{_cond}%::value}
												set {_d} to {ga::specs::%{_ga}%::conditions::%{_cond}%::description}
												message "{@Prefix} <tooltip:{@SeeCondTooltip}>{@SeeCond}"
									else:
										message "{@Prefix} {@ErrorPrefix} {@NoConditionSet}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Condition}"
					else:
						message "{@NoPermission}"
				else if arg-2 is "set":
					if player has permission "{@PermissionModifyCondition}" or "{@PermissionAll}":
						if check [arg-3 is set]->[arg-4 is set]->[arg-5 is set]:
							if GiveAll_gaExist(arg-3):
								set {_ga} to arg-3
								set {_cond} to arg-4
								set {_val} to arg-5
								if {_cond} is "in-region" or "not-in-region":
									set {_val} to ({_val} in lower case)
								if {_cond} is "min-co" or "max-co":
									if ({_val} parsed as integer) is not an integer:
										message "{@Prefix} {@ErrorPrefix} {@TimeIsInvalid}"
										stop
								loop {ga::debug::conds::*}:
									if loop-value is {_cond}:
										GiveAll_addCondition({_ga}, {_cond}, {_val})
										set {_cond} to {ga::specs::%{_ga}%::conditions::%{_cond}%::fullName}
										message "{@Prefix} {@ConditionSet}"
										stop
								message "{@Prefix} {@ErrorPrefix} {@WrongCondition}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Condition}"
					else:
						message "{@NoPermission}"
				else if arg-2 is "remove" or "rem" or "delete" or "del":
					if player has permission "{@PermissionModifyCondition}" or "{@PermissionAll}":
						if check [arg-3 is set]->[arg-4 is set]:
							if GiveAll_gaExist(arg-3):
								set {_ga} to arg-3
								set {_cond} to arg-4
								if {_cond} is "all":
									loop {ga::debug::conds::*}:
										GiveAll_removeCondition({_ga}, "%loop-value%")
									message "{@Prefix} {@ConditionCleared}"
									stop
								else:
									loop {ga::debug::conds::*}:
										if loop-value is {_cond}:
											GiveAll_removeCondition({_ga}, {_cond})
											set {_cond} to {ga::specs::%{_ga}%::conditions::%{_cond}%::fullName}
											message "{@Prefix} {@ConditionUnset}"
											stop
								message "{@Prefix} {@ErrorPrefix} {@WrongCondition}"
								message "%nl%{@Prefix} {@RemoveConditionInfo}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Condition}"
					else:
						message "{@NoPermission}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Condition}"
			else:
				message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Condition}"

		#-----------------------------------------------------#
		# 					   Blacklist					  #
		#-----------------------------------------------------#
		else if arg-1 is "blacklist" or "black" or "bl":
			if player has permission "{@PermissionBlackList}" or "{@PermissionAll}":
				if arg-2 is "add":
					if check [arg-3 is set]->[arg-4 is set]:
						set {_p} to arg-3
						if realPlayer(arg-3):
							if (arg-3 parsed as offline player) doesn't have permission "{@PermissionExemptBlackList}":
								if GiveAll_gaExist(arg-4):
									if GiveAll_isBlacklist(arg-3, arg-4) is not true:
										set {_ga} to arg-4
										add arg-3 to {ga::specs::%{_ga}%::blacklist::*}
										add arg-4 to {ga::blacklist::player::%{_p}%::*}
										message "{@Prefix} {@AddedPlayerToBlacklist}"
										if {@DoNotifyVictimOnBlacklist}:
											set {_e} to player
											message "{@Prefix} {@NotifyVictimOnBlacklist}" to {_p}
									else:
										message "{@Prefix} {@ErrorPrefix} {@PlayerAlreadyBlacklist}"
								else:
									message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@PlayerIsExemptedOfBlacklist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@InvalidPlayer}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_BlacklistAdd}"
				else if arg-2 is "remove" or "rem" or "delete" or "del":
					if check [arg-3 is set]->[arg-4 is set]:
						set {_p} to arg-3
						if realPlayer(arg-3):
							if GiveAll_gaExist(arg-4):
								if GiveAll_isBlacklist(arg-3, arg-4):
									set {_ga} to arg-4
									remove arg-3 from {ga::specs::%{_ga}%::blacklist::*}
									remove arg-4 from {ga::blacklist::player::%{_p}%::*}
									set {_a::*} to {ga::specs::%{_ga}%::blacklist::*}
									clear {ga::specs::%{_ga}%::blacklist::*}
									set {ga::specs::%{_ga}%::blacklist::*} to {_a::*}
									set {_b::*} to {ga::blacklist::player::%{_p}%::*}
									clear {ga::blacklist::player::%{_p}%::*}
									set {ga::blacklist::player::%{_p}%::*} to {_b::*}
									message "{@Prefix} {@RemovedPlayerFromBlacklist}"
									if {@DoNotifyVictimOnUnblacklist}:
										set {_e} to player
										message "{@Prefix} {@NotifyVictimOnUnblacklist}" to {_p}
								else:
									message "{@Prefix} {@ErrorPrefix} {@PlayerAlreadyBlacklist}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@InvalidPlayer}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_BlacklistRemove}"

				else if arg-2 is "see" or "info" or "list":
					if arg-4 is set:
						if arg-3 is "p" or "player" or "j" or "joueur":
							set {_p} to arg-4
							if realPlayer(arg-4):
								if size of {ga::blacklist::player::%{_p}%::*} > 0:
									set {_bl} to "%{ga::blacklist::player::%{_p}%::*}%"
									replace "and" with "&r{@ComaColorInList}et&r{@GAColorInList}" in {_bl}
									replace "," with "&r{@ComaColorInList},&r{@GAColorInList}" in {_bl}
									message "{@Prefix} {@BlackListListP} %{_bl}%"
								else:
									message "{@Prefix} {@PlayerIsNotBlacklistFromAnyGiveAll}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@InvalidPlayer}"
						else if arg-3 is "ga" or "giveall" or "give-all":
							if GiveAll_gaExist(arg-4):
								set {_ga} to arg-4
								if size of {ga::specs::%{_ga}%::blacklist::*} > 0:
									set {_bl} to "%{ga::specs::%{_ga}%::blacklist::*}%"
									replace "and" with "&r{@ComaColorInList}et&r{@GAColorInList}" in {_bl}
									replace "," with "&r{@ComaColorInList},&r{@GAColorInList}" in {_bl}
									message "{@Prefix} {@BlackListListGA} %{_bl}%"
								else:
									message "{@Prefix} {@NoPlayerBlackListOfThisGA}"
							else:
								message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_BlacklistInfo}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_BlacklistInfo}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Blacklist}"
			else:
				message "{@NoPermission}"

		#-----------------------------------------------------#
		# 					     Group						  #
		#-----------------------------------------------------#
		else if arg-1 is "group" or "groupe" or "grp":
			if arg-2 is "create" or "make":
				if player has permission "{@PermissionGroupCreate}" or "{@PermissionGroupAll}" or "{@PermissionAll}":
					if arg-3 is set:
						if GiveAll_grpExist(arg-3) is not true:
							set {_grp} to arg-3 # PlaceHolder pour le message
							set {_n} to now formatted with "{@dateFormat}"
							GiveAll_newGroup(arg-3, 0, 10, "%player%", "%{_n}%") # Création du groupe
							message "{@Prefix} {@GroupCreated}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@GroupAlreadyExists}"
