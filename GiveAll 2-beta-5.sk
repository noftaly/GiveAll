function GiveAll_confirmDeleteGUI(p: player, type: text, e: text, cmd: text):
	set {_ga} to {_e}
	open virtual chest with 2 rows named "{@ConfirmDeleteGUI}" to {_p}
	format gui slot 4 of {_p} with book named "{@InfoGAName}"
	format gui slot 12 of {_p} with red dye named "{@CancelDelete}" to run:
		make {_p} execute command "/{@MainCommand} %{_cmd}%"
	format gui slot 14 of {_p} with slimeball named "{@ConfirmDelete}" to run:
		set {ga::player::%{_p}%::dispMode} to "GroupsAndGa"
		if {_type} is "ga":
			make {_p} execute command "/{@MainCommand} delete %{_e}%"
			if GiveAll_isLastGiveAll() is false:
				make {_p} execute command "/{@MainCommand} list gui"
				stop
			close {_p}'s inventory
		else if {_type} is "grp":
			make {_p} execute command "/{@MainCommand} group delete %{_e}%"

function GiveAll_listOptionsGUI(p: player, type: text, clicktype: text, e: text):
	if {_type} is "ga":
		if {_clicktype} is "gauche":
			if contains(GiveAll_current(), {_e}):
				make {_p} execute command "/{@MainCommand} stop %{_e}%"
			else:
				make {_p} execute command "/{@MainCommand} start %{_e}%"
			wait 0.5 seconds
			make {_p} execute command "/{@MainCommand} list gui"
		else if {_clicktype} is "middle":
			make {_p} execute command "/{@MainCommand} edititem %{_e}%"
		else if {_clicktype} is "shiftgauche":
			make {_p} execute command "/{@MainCommand} modify %{_e}%"
		else if {_clicktype} is "shiftdroit":
			make {_p} execute command "/{@MainCommand} delete %{_e}%"
			set {ga::player::%{_p}%::dispMode} to "GroupsAndGa"
			if GiveAll_isLastGiveAll() is false:
				make {_p} execute command "/{@MainCommand} list gui"
				stop
			close {_p}'s inventory
	else if {_type} is "grp":
		if {_clicktype} is "gauche":
			make {_p} execute command "/{@MainCommand} group info %{_e}% gui"
		else if {_clicktype} is "middle":
			make {_p} execute command "/{@MainCommand} list gui"
		else if {_clicktype} is "shiftdroit":
			make {_p} execute command "/{@MainCommand} group delete %{_e}%"
			set {ga::player::%{_p}%::dispMode} to "GroupsAndGa"
			if GiveAll_isLastGiveAll() is false:
				make {_p} execute command "/{@MainCommand} list gui"
				stop
			close {_p}'s inventory
	if {_clicktype} is "droit":
		GiveAll_confirmDeleteGUI({_p}, "%{_type}%", "%{_e}%", "list gui")

function GiveAll_conditionGUI(p: player, ga: text, modify: boolean=false):
	if {_modify}:
		open virtual chest with 4 rows named "{@ModifyConditionGUI}" to {_p}
		format gui slot 35 of {_p} with arrow named "{@BackItem}" to run:
			make {_p} execute command "/ga modify %{_ga}%"
	else:
		open virtual chest with 3 rows named "{@ModifyConditionGUI}" to {_p}
	format gui slot 4 of {_p} with book named "{@InfoGAName}"
	set {_i} to 9
	loop {ga::debug::conds::*}:
		set {_cond} to loop-value
		set {_name} to "&2%{ga::debug::conds::%{_cond}%::fullName}%"
		set {_desc} to "&7%{ga::debug::conds::%{_cond}%::description}%"
		replace all "(requiert" with "&7&oRequiert" in {_desc}
		replace all ": /ga api)" with ": /ga api" in {_desc}
		set {_lore} to "&c&oDésactivé"
		if {ga::specs::%{_ga}%::conditions::%{_cond}%::isEnable}:
			set {_lore} to "&a&oActivé &7&o➟ %{ga::specs::%{_ga}%::conditions::%{_cond}%::value}%"
		format gui slot {_i} of {_p} with name tag named {_name} with lore "%{_lore}%", "" and "%{_desc}%" to run:
			close {_p}'s inventory
			message "&r%nl%&r%nl%{@Prefix} <tooltip:{@TTPClickForAction}><suggest command:/ga cond set %{_ga}% %{_cond}% Valeur %(""en secondes"") if {_cond} is ""min-co"" or ""max-co"", else """"%>{@MSGClickForAction}&r%nl%&r%nl%&r" to {_p}
		add 1 to {_i}

function GiveAll_commandParams(type: text, arg.3: text, arg.4: text, arg.5: text) :: integer:
	if {_type} is "cd":
		loop all integers between 3 and 5:
			if {_arg.%loop-value%} contains "cd:":
				replace "cd:" with "" in {_arg.%loop-value%}
				set {_x} to ({_arg.%loop-value%} parsed as integer)
				if and(check [{_x} != 0], check [{_x} < 10]):
					set {_x} to 10
		return {_x}
	else if {_type} is "t":
		loop all integers between 3 and 5:
			if {_arg.%loop-value%} contains "t:":
				replace "t:" with "" in {_arg.%loop-value%}
				set {_x} to ({_arg.%loop-value%} parsed as integer)
				set {_x} to abs({_x})
		return {_x}

command /ag [<text>] [<text>] [<text>] [<text>] [<text>]:
	#aliases: {@AliasesCommands}
	executable by: player and console
	trigger:
		#-----------------------------------------------------#
		# 						Start 						  #
		#-----------------------------------------------------#
		if arg-1 is "start":
			if player has permission "{@PermissionStart}" or "{@PermissionAll}":
				if GiveAll_gaExist(arg-2):
					if contains(GiveAll_current(), arg-2) is not true:
						set {_i} to false # {_i} définit si le giveall va se lancer instantanément ou pas.
						set {_ga} to arg-2
						add {_ga} to {ga::current::*}
						set {ga::current::%{_ga}%::time} to 0
						set {ga::current::%{_ga}%::status} to "Countdown"
						set {ga::specs::%{_ga}%::status} to "Countdown"
						set {ga::current::%{_ga}%::startExecutor} to player
						set {ga::current::%{_ga}%::startDate} to now formatted with "{@dateFormat}"
						set {_cd} to GiveAll_commandParams("cd", arg-3, arg-4, arg-5)
						set {_t} to GiveAll_commandParams("t", arg-3, arg-4, arg-5)
						set {_i} to ("%(""true"") if {_cd} is 0, else (""false"")%" parsed as boolean)
						if {_i} is false: # S'il ne se lance pas instantanément, alors
							set {_tl} to {ga::specs::%{_ga}%::countdown} # Algorythme compliqué pour faire le compte a rebours
							if {_cd} is set:
								set {_tl} to {_cd}
							if {_tl} mod 10 != 0:
								broadcast "{@Prefix} {@GAStartsInXSeconds}"
								play sound "{@TimerSound}" at all players
							while true:
								if and(check [{_tl} mod 10 = 0], check [GiveAll_isInProgress({_ga})]):
									broadcast "{@Prefix} {@GAStartsInXSeconds}"
									play sound "{@TimerSound}" at all players
								wait 1 second
								remove 1 from {_tl}
								if {_tl} < 10:
									set {_tl} to 10
									exit loop
							remove 5 from {_tl}
							wait 5 seconds
							loop 5 times:
								if GiveAll_isInProgress({_ga}):
									broadcast "{@Prefix} {@GAStartsInXSeconds}"
									play sound "{@TimerSound}" at all players
									remove 1 from {_tl}
									wait 1 second
								else:
									stop
						if GiveAll_isInProgress({_ga}):
							set {ga::current::%{_ga}%::status} to "In progress"
							set {ga::specs::%{_ga}%::status} to "In progress"
							broadcast "{@Prefix} {@GAStarts}"
							play sound "{@TimerSound}" at all players
							while GiveAll_isInProgress({_ga}):
								loop all players:
									if GiveAll_playerCanReceiveItems(loop-player, {_ga}):
										set {_x} to a random element out of {ga::specs::%{_ga}%::items::*}
										if {ga::specs::%{_ga}%::droptype} is "give":
											give {_x} to loop-player
										else if {ga::specs::%{_ga}%::droptype} is "drop":
											set {_pos} to location at x-location of loop-player, y-location of loop-player+3, z-location of loop-player
											if block at {_pos} is not air:
												set {_pos} to location at x-location of loop-player, y-location of loop-player+1, z-location of loop-player
											drop {_x} at {_pos}
								loop random integer between {ga::debug::minGiveTime} and {ga::debug::maxGiveTime} times:
									wait 1 second
									add 1 to {ga::current::%{_ga}%::time}
									if and(check [{_t} is not set], check [{ga::specs::%{_ga}%::time} != 0], check [{ga::current::%{_ga}%::time} >= {ga::specs::%{_ga}%::time}]):
										broadcast "{@Prefix} {@GAIsFinish}"
										set {ga::specs::%{_ga}%::status} to "Stopped"
										clear {ga::current::%{_ga}%::*}
										remove {_ga} from {ga::current::*}
										stop
									else if and(check [{_t} != 0], check [{ga::current::%{_ga}%::time} >= {_t}]):
										broadcast "{@Prefix} {@GAIsFinish}"
										set {ga::specs::%{_ga}%::status} to "Stopped"
										clear {ga::current::%{_ga}%::*}
										remove {_ga} from {ga::current::*}
										stop
					else:
						message "{@Prefix} {@ErrorPrefix} {@GAIsAlreadyStarted}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Stop 						  #
		#-----------------------------------------------------#
		else if arg-1 is "stop":
			if player has permission "{@PermissionStop}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						set {_ga} to arg-2
						if contains(GiveAll_current(), {_ga}):
							GiveAll_stop({_ga})
							broadcast "{@Prefix} {@GAHasBeenStopped}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@NoGAInProgress}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Stop}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						GiveHand 					  #
		#-----------------------------------------------------#
		else if arg-1 is "givehand" or "gh":
			if player has permission "{@PermissionGiveHand}" or "{@PermissionAll}":
				if player's tool is not air:
					if amount of all players > 1:
						if arg-2 is set:
							set {_world} to arg-2
						loop all players:
							if loop-player is not player:
								if {_world} is set:
									if loop-player's world is {_world}:
										give player's tool to loop-player
								else:
									give player's tool to loop-player
						set {_item} to player's tool
						set {_number} to amount of all players
						message "{@Prefix} {@GAHandHasBeenGiven}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@Alone}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@TakeAnItem}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Create 						  #
		#-----------------------------------------------------#
		else if arg-1 is "create" or "make":
			if player has permission "{@PermissionCreate}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2) is not true:
						set {_ga} to arg-2 # PlaceHolder pour le message
						set {_time} to 0 # Durée du GiveAll
						set {_cd} to 10 # Compte à rebours du GiveAll
						if or(check [arg-3 is set], check [arg-4 is set]): # Si un argument est mis, alors on peut regarder si c'est le compte a rebours ou la durée
							set {_cd} to GiveAll_commandParams("cd", arg-3, arg-4, "") # Compte a rebours
							set {_time} to GiveAll_commandParams("t", arg-3, arg-4, "") # Durée
						set {_date} to now formatted with "{@dateFormat}" # Date
						GiveAll_newGiveAll(arg-2, "{@defaultGroup}", {_time}, {_cd}, "give", "%player%", {_date}, (diamond sword of sharpness 2 and 10 stone)) # Création du GiveAll
						message "{@Prefix} {@GACreated}"
						message "{@Prefix} {@GACreationInfosTime}"
						message "{@Prefix} {@GACreationInfosCD}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GaAlreadyExists}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Create}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Delete 						  #
		#-----------------------------------------------------#
		else if arg-1 is "delete" or "del" or "remove" or "rem":
			if player has permission "{@PermissionDelete}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						if contains(GiveAll_current(), arg-2) is not true:
							GiveAll_deleteGiveAll(arg-2)
							set {_ga} to arg-2
							message "{@Prefix} {@GADeleted}"
						else:
							message "{@Prefix} {@CantModifyBecauseGAIsInProgress}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Delete}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Purge 						  #
		#-----------------------------------------------------#
		else if arg-1 is "purge":
			if player has permission "{@PermissionPurge}" or "{@PermissionAll}":
				if GiveAll_isLastGiveAll() is not true:
					set {_total} to size of {ga::list::*}
					if {ga::player::%player%::purge}:
						clear {ga::player::%player%::purge}
						GiveAll_purgeGiveAll()
						message "{@Prefix} {@SuccessfullyPurged}"
					else:
						set {ga::player::%player%::purge} to true
						message "{@Prefix} {@PleaseConfirmPurge}"
						wait {@purgeConfirmExpire} seconds
						clear {ga::player::%player%::purge}
				else:
					message "{@Prefix} {@ErrorPrefix} {@GAListIsEmpty}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Rename 						  #
		#-----------------------------------------------------#
		else if arg-1 is "rename":
			if player has permission "{@PermissionRename}" or "{@PermissionAll}":
				if GiveAll_gaExist(arg-2):
					set {_ga} to arg-2
					set {_newName} to arg-3
					if GiveAll_gaExist(arg-3) is not true:
						if contains(GiveAll_current(), arg-2) is not true:
							GiveAll_renameGiveAll(arg-2, arg-3)
							message "{@Prefix} {@RenamedSuccessfully}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@CantModifyBecauseGAIsInProgress}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@NewGAAlreadyExists}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						List 						  #
		#-----------------------------------------------------#
		else if arg-1 is "list":
			if player has permission "{@PermissionChatList}" or "{@PermissionGUIList}" or "{@PermissionAll}":
				set {_number} to size of {ga::list::*}
				set {_gnum} to size of {ga::glist::*}
				if and(check [{_number} = 0], check [{_gnum} = 0]):
					message "{@Prefix} {@GAListIsEmpty}"
				else if arg-2 is "gui":
					if player has permission "{@PermissionGUIList}" or "{@PermissionAll}":
						if arg-3 is not set:
							set {_p} to 1
						else:
							set {_p} to arg-3 parsed as integer
						if {_p} is not an integer:
							set {_p} to 1
						else:
							set {_p} to abs({_p})
						set {_mp} to ceil(size of {ga::list::*} / 36)
						if {_mp} is 0:
							set {_mp} to 1
						if {_p} > {_mp}:
							make player execute command "/{@MainCommand} list gui %{_mp}%"
							stop
						open virtual chest with 6 rows named "{@TitleGUIList}" to player
						set {_i} to 0
						set {_d} to {ga::player::%player%::dispMode}
						if {_d} is "GroupsAndGa" or "allFullGroups" or "onlyGroup":
							loop {ga::glist::*}:
								set {_grp} to loop-value
								set {_s} to size of {ga::gspecs::%{_grp}%::givealls::*}
								if {_d} is "allFullGroups":
									if {_s} is 0:
										continue
									if {_s} is not set:
										continue
								if {_i} >= {_p} * 36 - 36:
									if {_i} < {_p} * 36:
										loop {ga::gspecs::%{_grp}%::givealls::*}:
											set {_dispInGroup::%loop-value-2%} to true # On ajoute le GiveAll à une liste, pour après éviter de l'afficher en dehors des groupes
										set {_nbga} to size of {ga::gspecs::%{_grp}%::givealls::*}
										set {_cd} to {ga::gspecs::%{_grp}%::countdown}
										set {_time} to {ga::gspecs::%{_grp}%::time}
										set {_creator} to {ga::gspecs::%{_grp}%::infos::creator}
										set {_date} to {ga::gspecs::%{_grp}%::infos::date}
										set {_action} to "{@LeftClickActionStartGroup}"
										loop {ga::gspecs::%{_grp}%::givealls::*}:
											if size of {ga::specs::%loop-value-2%::items::*} is 0:
												set {_action} to "{@LeftClickActionCantStartItemGroup}"
										if {_s} is 0:
											set {_action} to "{@LeftClickActionCantStartGAGroup}"
										set {_l} to "{@LoreGAListGroup}"
										set {_lore::*} to {_l} split at "||"
										#if {_action} is "{@LeftClickActionStartGroup}":
										# Middle click / Lancer
										format gui slot {_i} - 36 * ({_p} - 1) of player with a book named "{@GAListItemGroup}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "grp", "middle", "%loop-value%") with middle mouse button
										# Right click / Supprimer avec confirmation
										format gui slot {_i} - 36 * ({_p} - 1) of player with a book named "{@GAListItemGroup}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "grp", "droit", "%loop-value%") with right mouse button
										# Shift right click / Supprimer sans confirmation
										format gui slot {_i} - 36 * ({_p} - 1) of player with a book named "{@GAListItemGroup}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "grp", "shiftdroit", "%loop-value%") with right mouse button with shift
										# Left click / Gérer le contenu
										format gui slot {_i} - 36 * ({_p} - 1) of player with a book named "{@GAListItemGroup}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "grp", "gauche", "%loop-value%") with left mouse button
								add 1 to {_i}
						if {_d} is not "onlyGroup":
							loop {ga::list::*}:
								if {_d} is "GroupsAndGa": # Si on affiche par groupe...
									if {_dispInGroup::%loop-value%}:
										continue
								if {_i} >= {_p} * 36 - 36:
									if {_i} < {_p} * 36:
										set {_ga} to loop-value
										set {_nbitem} to size of {ga::specs::%{_ga}%::items::*}
										set {_time} to "%{ga::specs::%{_ga}%::time}%"
										set {_cd} to {ga::specs::%{_ga}%::countdown}
										set {_conds} to GiveAll_activeConditions({_ga})
										set {_creator} to {ga::specs::%{_ga}%::infos::creator}
										set {_date} to {ga::specs::%{_ga}%::infos::date}
										set {_statut} to "{@StatusStop}"
										set {_action} to "{@LeftClickActionCantStart}"
										if {ga::specs::%{_ga}%::status} is "Countdown":
											set {_action} to "{@LeftClickActionStop}"
											set {_statut} to "{@StatusInCountDown}"
										else if {ga::specs::%{_ga}%::status} is "In Progress":
											set {_action} to "{@LeftClickActionStop}"
											set {_statut} to "{@StatusInProgress}"
										else if size of {ga::specs::%{_ga}%::items::*} > 0:
											set {_action} to "{@LeftClickActionStart}"
										if {ga::specs::%{_ga}%::time} is 0:
											set {_time} to "{@InfiniteTimeForGUIList}"
										set {_blacklist} to 0
										if size of {ga::specs::%{_ga}%::blacklist::*} > 0:
											set {_blacklist} to size of {ga::specs::%{_ga}%::blacklist::*}
										set {_l} to "{@LoreGAList}"
										set {_lore::*} to {_l} split at "||"
										if {_action} is "{@LeftClickActionStop}" or "{@LeftClickActionStart}":
											format gui slot {_i} - 36 * ({_p} - 1) of player with a paper named "{@GAListItem}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "ga", "gauche", "%loop-value%") with left mouse button
										format gui slot {_i} - 36 * ({_p} - 1) of player with a paper named "{@GAListItem}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "ga", "droit", "%loop-value%") with right mouse button
										format gui slot {_i} - 36 * ({_p} - 1) of player with a paper named "{@GAListItem}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "ga", "shiftdroit", "%loop-value%") with right mouse button with shift
										format gui slot {_i} - 36 * ({_p} - 1) of player with a paper named "{@GAListItem}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "ga", "middle", "%loop-value%") with middle mouse button
										format gui slot {_i} - 36 * ({_p} - 1) of player with a paper named "{@GAListItem}" with lore {_lore::*} to run function GiveAll_listOptionsGUI(player, "ga", "shiftgauche", "%loop-value%") with left mouse button with shift
								add 1 to {_i}
						set {_l1} to "{@ToggleDisplayModeGroupAndGiveall}"
						set {_l2} to "{@ToggleDisplayModeOnlyGroups}"
						set {_l3.1} to "{@ToggleDisplayModeOnlyFullGroupsAndGiveall1}"
						set {_l3.2} to "{@ToggleDisplayModeOnlyFullGroupsAndGiveall2}"
						set {_l4} to "{@ToggleDisplayModeOnlyGiveall}"
						format gui slot 45 of player with a book named "{@ToggleDisplayModeInGuiListName}" with lore "", "%(""&2"") if {_d} is ""GroupsAndGa"", else (""&7"")%%{_l1}%", "%(""&2"") if {_d} is ""onlyGroup"", else (""&7"")%%{_l2}%", "%(""&2"") if {_d} is ""allFullGroups"", else (""&7"")%%{_l3.1}%", "%(""&2"") if {_d} is ""allFullGroups"", else (""&7"")%%{_l3.2}%" and "%(""&2"") if {_d} is ""onlyGiveAll"", else (""&7"")%%{_l4}%" to run with right mouse button:
							if {ga::player::%player%::dispMode} is "onlyGroup":
								set {ga::player::%player%::dispMode} to "GroupsAndGa"
							else if {ga::player::%player%::dispMode} is "allFullGroups":
								set {ga::player::%player%::dispMode} to "onlyGroup"
							else if {ga::player::%player%::dispMode} is "onlyGiveAll":
								set {ga::player::%player%::dispMode} to "allFullGroups"
							else if {ga::player::%player%::dispMode} is "GroupsAndGa":
								set {ga::player::%player%::dispMode} to "onlyGiveAll"
							make player execute command "/{@MainCommand} list gui %{_p}%"
						format gui slot 45 of player with a book named "{@ToggleDisplayModeInGuiListName}" with lore "", "%(""&2"") if {_d} is ""GroupsAndGa"", else (""&7"")%%{_l1}%", "%(""&2"") if {_d} is ""onlyGroup"", else (""&7"")%%{_l2}%", "%(""&2"") if {_d} is ""allFullGroups"", else (""&7"")%%{_l3.1}%", "%(""&2"") if {_d} is ""allFullGroups"", else (""&7"")%%{_l3.2}%" and "%(""&2"") if {_d} is ""onlyGiveAll"", else (""&7"")%%{_l4}%" to run with left mouse button:
							if {ga::player::%player%::dispMode} is "GroupsAndGa":
								set {ga::player::%player%::dispMode} to "onlyGroup"
							else if {ga::player::%player%::dispMode} is "onlyGroup":
								set {ga::player::%player%::dispMode} to "allFullGroups"
							else if {ga::player::%player%::dispMode} is "allFullGroups":
								set {ga::player::%player%::dispMode} to "onlyGiveAll"
							else if {ga::player::%player%::dispMode} is "onlyGiveAll":
								set {ga::player::%player%::dispMode} to "GroupsAndGa"
							make player execute command "/{@MainCommand} list gui %{_p}%"
						if {_p} != {_mp}:
							format gui slot 50 of player with an arrow named "{@NextPage}" to run player command "/{@MainCommand} list gui %({_p} + 1)%"
						if {_p} != 1:
							format gui slot 48 of player with an arrow named "{@PreviousPage}" to run player command "/{@MainCommand} list gui %({_p} - 1)%"
						format gui slot 49 of player with name tag named "{@InfoPageItem}" with lore "{@LoreInfoPageItem}"
						format gui slot 36, 38, 40, 42 and 44 of player with white glass pane named "&r"
						format gui slot 37, 39, 41 and 43 of player with blue glass pane named "&r"
						format gui slot 53 of player with a barrier named "{@CloseGUIItem}" to close
					else:
						message "{@NoPermission}"
				else:
					set {_gal} to ""
					set {_i} to 1
					loop {ga::list::*}:
						set {_ga} to "{@GAColorInList}%loop-value%"
						if contains(GiveAll_current(), loop-value):
							set {_ga} to "{@GAColorInList}{@GAColorInListIfInProgress}%loop-value%"
						set {_gal} to "%{_gal}%%(""{@ComaColorInList}, "") if {_i} is not 1, else ("""")%%{_ga}%"
						add 1 to {_i}
					message "{@Prefix} {@GAList} %{_gal}%"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Info 						  #
		#-----------------------------------------------------#
		else if arg-1 is "info" or "infos" or "information" or "informations":
			if player has permission "{@PermissionInfo}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						set {_ga} to arg-2
						message "{@Prefix} {@InfoHeader}"
						#set {_a} to 
						message "{@Prefix} {@ComaColorInList}Durée :{@GAColorInList} %({@InfiniteTimeForGUIList}) if {ga::specs::%{_ga}%::time} is 0 else {ga::specs::%{_ga}%::time}%"
						message "{@Prefix} {@ComaColorInList}Compte à rebours :{@GAColorInList} %{ga::specs::%{_ga}%::countdown}% secs"
						message "{@Prefix} {@ComaColorInList}Don des items :{@GAColorInList} %{ga::specs::%{_ga}%::droptype}%"
						set {_a} to ("aucun") if {ga::specs::%{_ga}%::group} is "{@defaultGroup}" else "%{ga::specs::%{_ga}%::group}%"
						message "{@Prefix} {@ComaColorInList}Groupe :{@GAColorInList} %{_a}%"
						set {_statut} to "{@StatusStop}"
						if {ga::specs::%{_ga}%::status} is "Countdown":
							set {_statut} to "{@StatusInCountDown}"
						else if {ga::specs::%{_ga}%::status} is "In Progress":
							set {_statut} to "{@StatusInProgress}"
						message "{@Prefix} {@ComaColorInList}Statut :{@GAColorInList} %{_statut}%"
						message "{@Prefix} {@ComaColorInList}Créateur :{@GAColorInList} %{ga::specs::%{_ga}%::infos::creator}%"
						message "{@Prefix} {@ComaColorInList}Création :{@GAColorInList} %{ga::specs::%{_ga}%::infos::date}%"
						set {_cond} to ""
						loop {ga::specs::%{_ga}%::conditions::*}:
							if {ga::specs::%{_ga}%::conditions::%loop-value%::isEnable}:
								set {_cond} to "%{_cond}%, %{ga::debug::conds::%loop-value%::fullName}%"
						set {_a} to ("aucune") if {_cond} is "" else "%{_cond}%"
						message "{@Prefix} {@ComaColorInList}Conditions :{@GAColorInList} %{_a}%"
						message "{@Prefix} {@ComaColorInList}Items :{@GAColorInList} <tooltip:&7%{ga::specs::%{_ga}%::items::*}%>%size of {ga::specs::%{_ga}%::items::*}%<reset>"

					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Info}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Add 						  #
		#-----------------------------------------------------#
		else if arg-1 is "add" or "addi" or "additem" or "additems":
			if player has permission "{@PermissionAddItem}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						if player's tool is not air:
							add player's tool to {ga::specs::%arg-2%::items::*}
							set {_ga} to arg-2
							set {_item} to player's tool
							message "{@Prefix} {@ItemAdded}"
						else:
							message "{@Prefix} {@ErrorPrefix} {@TakeAnItem}"
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_Add}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Edit 						  #
		#-----------------------------------------------------#
		else if arg-1 is "edit" or "edititems" or "edititem":
			if player has permission "{@PermissionEditItem}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						set {_ga} to arg-2
						if arg-3 is not set:
							set {_p} to 1
						else:
							set {_p} to arg-3 parsed as integer
						if {_p} is not an integer:
							set {_p} to 1
						else:
							set {_p} to abs({_p})
						set {_mp} to ceil(size of {ga::specs::%{_ga}%::items::*} / 36)
						if {_mp} is 0:
							set {_mp} to 1
						if {_p} > {_mp}:
							make player execute command "/{@MainCommand} edititems %{_ga}% %{_mp}%"
							stop
						set {ga::player::%player%::editingItems} to "%{_ga}%"
						open virtual chest with 6 rows named "{@EditItems}" to player
						set {_in} to 0
						loop {ga::specs::%{_ga}%::items::*}:
							if {_in} >= {_p} * (36) - 36:
								if {_in} < {_p} * 36:
									set {_i} to loop-value
									format gui slot {_in} - 36 * ({_p} - 1) of player with loop-value to run with right mouse button:
										GiveAll_removeItem({_ga}, {_i})
										wait 1 tick
										make player execute command "/{@MainCommand} edititems %{_ga}% %{_p}%"
							add 1 to {_in}
						if {_p} != {_mp}:
							format gui slot 50 of player with an arrow named "{@NextPage}" to run player command "/{@MainCommand} edititems %{_ga}% %({_p} + 1)%"
						if {_p} != 1:
							format gui slot 48 of player with an arrow named "{@PreviousPage}" to run player command "/{@MainCommand} edititems %{_ga}% %({_p} - 1)%"
						format gui slot 36, 38, 40, 42 and 44 of player with white glass pane named "&r"
						format gui slot 37, 39, 41 and 43 of player with blue glass pane named "&r"
						format gui slot 53 of player with a barrier named "{@CloseGUIItem}" to close
						if player has permission "{@PermissionGUIList}" or "{@PermissionAll}":
							format gui slot 45 of player with a paper named "{@BackItem}" to run player command "/{@MainCommand} list gui"
						if player has permission "{@PermissionClearGA}" or "{@PermissionAll}":
							format gui slot 51 of player with a redstone dust item named "{@ClearGA}" to run:
								make player execute command "/{@MainCommand} clear %{_ga}%"
								wait 1 tick
								make player execute command "/{@MainCommand} edititems %{_ga}% %{_p}%"
						if player has permission "{@PermissionAddItem}" or "{@PermissionAll}":
							format gui slot 49 of player with gold block named "{@AddItemToGA}" to run:
								if gui-cursor is an item:
									if gui-cursor is not air:
										set {ga::player::%player%::dropToAdd} to true
										add gui-cursor to {ga::specs::%{_ga}%::items::*}
										make player execute command "/{@MainCommand} edititems %{_ga}% %{_p}%"
										clear {ga::player::%player%::dropToAdd}
					else:
						message "{@Prefix} {@ErrorPrefix} {@GADoesntExist}"
				else:
					message "{@Prefix} {@ErrorPrefix} {@WrongCommandSyntax_EditItem}"
			else:
				message "{@NoPermission}"
		#-----------------------------------------------------#
		# 						Modify 						  #
		#-----------------------------------------------------#
		else if arg-1 is "modify":
			if player has permission "{@PermissionModify}" or "{@PermissionAll}":
				if arg-2 is set:
					if GiveAll_gaExist(arg-2):
						set {_ga} to arg-2
						open virtual chest with 6 rows named "{@ModifyGA}" to player
						format gui slot 4 of player with a book named "{@InfoGAName}"
						format gui slot 10 of player with a red glass pane named "{@NotEnoughPermissionInGUI}"
						if player has permission "{@PermissionDuplicate}" or "{@PermissionAll}":
							format gui slot 10 of player with a name tag named "{@GAModifyDuplicate}" to run:
								make player execute command "/{@MainCommand} duplicate %{_ga}%"
						format gui slot 11 of player with a red glass pane named "{@NotEnoughPermissionInGUI}"
						if player has permission "{@PermissionRename}" or "{@PermissionAll}":
							format gui slot 11 of player with a book and quill named "{@GAModifyRename}" to run:
								close player's inventory
								message "&r%nl%&r%nl%<tooltip:{@TTPClickForAction}><suggest command:/ga rename %{_ga}% MonNouveauGiveAll>{@Prefix} <tooltip:{@TTPClickForAction}><suggest command:/ga rename %{_ga}% MonNouveauGiveAll>{@MSGClickForAction}&r%nl%&r%nl%&r"
						format gui slot 12 of player with a red glass pane named "{@NotEnoughPermissionInGUI}"
						if player has permission "{@PermissionEditItem}" or "{@PermissionAll}":
							format gui slot 12 of player with a grass block named "{@GAEditItems}" to run:
								make player execute command "/{@MainCommand} edititem %{_ga}% 1"
						format gui slot 13 of player with a red glass pane named "{@NotEnoughPermissionInGUI}"
						if player has permission "{@PermissionModifyCondition}" or "{@PermissionAll}":
							format gui slot 13 of player with iron bar named "{@ModifyCondition}" to run function GiveAll_conditionGUI(player, "%{_ga}%", true)
						format gui slot 14 of player with a red glass pane named "{@NotEnoughPermissionInGUI}"
						if player has permission "{@PermissionDelete}" or "{@PermissionAll}":
							format gui slot 14 of player with a tnt named "{@DeleteInModifyGUI}" to run function GiveAll_confirmDeleteGUI(player, "ga", "%{_ga}%", "modify %{_ga}%")
